<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	version="3.1">

	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>

	<filter>
		<filter-name>AuthFilter</filter-name>
		<filter-class>com.ems.filter.AuthFilter</filter-class>
	</filter>

	<filter-mapping>
		<filter-name>AuthFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	


</web-app>
